extends layout

block main
  main
    h1.h2= _name
    if _calendar && _calendarUrl
      p
        a(href=_calendarUrl)!= _calendar
    else if _calendar
      p!= _calendar
    else
      p no calendar
    if _calendarRank
      p
        em= _calendarRank
    div(style='background:url("medias/images/moons/' + _moonPhase + '.svg");background-size:cover;width:7rem;height:7rem')
    include includes/links
